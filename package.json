{
  "name": "multi-agent-github-automation",
  "version": "1.0.0",
  "description": "Advanced multi-agent GitHub automation system with parallel AI workflows",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "build": "node scripts/build.js",
    "docker:build": "docker build -t github-automation .",
    "docker:run": "docker run -p 3000:3000 github-automation",
    "agents:thinking": "node src/agents/thinking-agent.js",
    "agents:action": "node src/agents/action-agent.js",
    "integrations:qodo": "node src/integrations/qodo-integration.js",
    "integrations:copilot": "node src/integrations/copilot-enhancer.js",
    "browser:automation": "node src/browser-automation/github-ui-automation.js",
    "setup:env": "node scripts/setup-environment.js",
    "health:check": "node scripts/health-check.js"
  },
  "keywords": [
    "github",
    "automation",
    "ai",
    "multi-agent",
    "workflow",
    "claude",
    "gpt",
    "qodo",
    "copilot",
    "browser-automation",
    "pull-request",
    "code-review"
  ],
  "author": {
    "name": "vivvek99",
    "url": "https://github.com/vivvek99"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vivvek99/min-browser-enhancement-suite.git"
  },
  "bugs": {
    "url": "https://github.com/vivvek99/min-browser-enhancement-suite/issues"
  },
  "homepage": "https://github.com/vivvek99/min-browser-enhancement-suite#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@octokit/rest": "^20.0.2",
    "@octokit/webhooks": "^12.0.10",
    "@octokit/auth-app": "^6.0.1",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "winston": "^3.11.0",
    "redis": "^4.6.10",
    "bull": "^4.12.2",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^4.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "openai": "^4.20.1",
    "node-cron": "^3.0.3",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "yaml": "^2.3.4",
    "semver": "^7.5.4",
    "glob": "^10.3.10",
    "fs-extra": "^11.1.1",
    "zod": "^3.22.4",
    "neverthrow": "^6.1.0"
  },
  "devDependencies": {
    "playwright": "^1.40.0",
    "puppeteer": "^21.5.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "@types/node": "^20.9.0",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "@types/jest": "^29.5.8",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.9",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "docker": ">=20.0.0",
    "kubernetes": ">=1.25.0"
  },
  "optionalDependencies": {
    "sharp": "^0.32.6",
    "canvas": "^2.11.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "browserslist": [
    "node >= 18"
  ],
  "config": {
    "agents": {
      "thinking": {
        "model": "claude-3-sonnet",
        "maxTokens": 4096,
        "temperature": 0.1
      },
      "action": {
        "model": "gpt-4",
        "maxTokens": 2048,
        "temperature": 0.3
      }
    },
    "integrations": {
      "qodo": {
        "enabled": true,
        "endpoint": "https://api.qodo.ai"
      },
      "copilot": {
        "enabled": true,
        "model": "gpt-4-turbo"
      },
      "browser": {
        "headless": true,
        "timeout": 30000
      }
    },
    "monitoring": {
      "enabled": true,
      "metricsInterval": 60000,
      "healthCheckInterval": 30000
    }
  }
}
